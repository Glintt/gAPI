{
  "name": "dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "bootstrap": "^4.1.1",
    "chart.js": "^2.7.2",
    "clipboard": "^2.0.1",
    "css-loader": "^0.28.11",
    "ejs": "^2.6.1",
    "express": "4.16.3",
    "hchs-vue-charts": "^1.2.8",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.1",
    "socket.io-client": "^2.1.0",
    "uws": "^9.14.0",
    "vue": "^2.5.13",
    "vue-chartjs": "^3.3.1",
    "vue-resource": "^1.5.0",
    "vue-router": "^3.0.1",
    "vuetable": "^1.5.12",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.9",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.9",
    "@vue/cli-service": "^3.0.0-beta.9",
    "@vue/eslint-config-prettier": "^3.0.0-beta.9",
    "babel-plugin-transform-inline-environment-variables": "^0.3.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^6.0.6",
    "vue-template-compiler": "^2.5.13"
  },
  "babel": {
    "presets": [
      "@vue/app",
      [
        "@babel/preset-stage-2",
        {
          "decoratorsLegacy": true
        }
      ]
    ],
    "plugins": [
      [
        "transform-inline-environment-variables",
        {
          "include": [
            "NODE_ENV",
            "API_HOST",
            "API_PORT",
            "SOCKET_HOST",
            "SOCKET_PORT",
            "API_PROTOCOL"
          ]
        }
      ]
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
